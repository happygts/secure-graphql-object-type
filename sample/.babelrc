{
  "presets": [
    ["env", {
      "targets": {
        "node": 6
      }
    }]
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-decorators-legacy",
    ["transform-runtime", {
      "helpers": false,
      "polyfill": true,
      "regenerator": false,
      "moduleName": "babel-runtime"
    }],
  ],
  "sourceMaps": true
}
